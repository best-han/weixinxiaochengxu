/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.mainBox{
  display: flex;
  flex-direction: column;
  background-color: #F2F2F2;
  height: 100%;
  position: relative;
}
.button {
  align-items: center;
  font-weight:bold;
  background-color:#C0C0C0;
}
.title {
  align-items: center;
  font-weight:bold;
  color:#00F;
}
.titleBox1{
  height:55px;
  width:90%;
  margin: 0 auto;
  border-radius: 5px;
  border: 1px solid #fff;
  color: #fff;
  background-color: #03A7F0;
  text-align: center;
  line-height: 55px;
  margin-top:10px
}